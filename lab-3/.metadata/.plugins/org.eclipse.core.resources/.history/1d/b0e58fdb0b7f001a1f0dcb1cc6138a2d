package Paint;
import java.util.*;
import java.util.List;
import java.util.ArrayList;;

public class Consola{

	public static void main(String[] args) {
		ArrayList<Shape> s = new ArrayList<>();
		
		
		Patrat p = new Patrat(2,"#b50aba",100);
		p.draw();
		p.getArea();
		p.getName();
		p.getHexColor();
		p.getBorderWidth();

		Dreptunghi d = new Dreptunghi(5,3,"#b50aba",90);
		d.draw();
		d.getArea();
		d.getName();
		d.getHexColor();
		d.getBorderWidth();
		
		Cerc c= new Cerc(5,"#b50aba",10);
		c.draw();
		c.getArea();
		c.getName();
		c.getHexColor();
		c.getBorderWidth();
		
	}

}
